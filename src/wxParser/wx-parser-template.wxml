
<template name="wx-parser-x">
  <block wx:if="{{item.node == 'element'}}">
    <block wx:if="{{item.tag == 'button'}}">
      <button type="default" size="mini" >
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="wx-parser-y" data="{{item}}"/>
        </block>
       </button>
    </block>
    <block wx:elif="{{item.tag == 'li' && item.parent == 'ol'}}">
      <view class="{{item.classStr}} wx-parser-ol-li">
        <view class="{{item.classStr}} wx-parser-ol-li-inner">
          <view class="{{item.classStr}} wx-parser-ol-li-text">
            <view class="{{item.classStr}}">{{item.order}}.</view>
          </view>
          <view class="{{item.classStr}} wx-parser-ol-li-text">
            <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
              <template is="wx-parser-y" data="{{item}}"/>
            </block>
          </view>
        </view>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'li' && item.parent == 'ul'}}">
      <view class="{{item.classStr}} wx-parser-li">
        <view class="{{item.classStr}} wx-parser-li-inner">
          <view class="{{item.classStr}} wx-parser-li-text">
            <view class="{{item.classStr}} wx-parser-li-circle"></view>
          </view>
          <view class="{{item.classStr}} wx-parser-li-text">
            <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
              <template is="wx-parser-y" data="{{item}}"/>
            </block>
          </view>
        </view>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'qqmusic'}}">
      <!-- <qqmusic> -->
    </block>
    <block wx:elif="{{item.classStr == 'db qqmusic_area'}}">
      <template is="wx-qqmusic" />
    </block>
    <block wx:elif="{{item.tag == 'video'}}">
      <template is="wx-parser-video" data="{{item}}"/>
    </block>
    <block wx:elif="{{item.tag == 'audio'}}">
      <template is="wx-parser-audio" data="{{item}}"/>
    </block>
    <block wx:elif="{{item.tag == 'img'}}">
      <template is="wx-parser-img" data="{{item}}"/>
    </block>
    <block wx:elif="{{item.tag == 'a'}}">
      <view class="wx-parser-inline {{item.classStr}} wx-parser-{{item.tag}}" data-href="{{item.attr.href}}"  style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="wx-parser-y" data="{{item}}"/>
        </block>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'table'}}">
      <view class="{{item.classStr}} wx-parser-{{item.tag}}" style="{{item.styleStr}}">
        <block  wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="wx-parser-y" data="{{item}}"/>
        </block>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'pre'}}">
      <view class="{{item.classStr}} wx-parser-{{item.tag}}" style="{{item.styleStr}}">
        <block  wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="wx-parser-y" data="{{item}}"/>
        </block>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'code'}}">
      <view class="{{item.classStr}} wx-parser-{{item.tag}}" style="{{item.styleStr}}">
        <block  wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="wx-parser-y" data="{{item}}"/>
        </block>
      </view>
    </block>
    <block wx:elif="{{item.tagType == 'block'}}">
      <view class="{{item.classStr}} wx-parser-{{item.tag}}" style="{{item.styleStr}}">
        <block  wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="wx-parser-y" data="{{item}}"/>
        </block>
      </view>
    </block>
    <view wx:else class="wx-parser-{{item.tag}} wx-parser-{{item.tagType}} {{item.classStr}}" style="{{item.styleStr}}">
      <block  wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
        <template is="wx-parser-y" data="{{item}}"/>
      </block>
    </view>
  </block>
  <block wx:elif="{{item.node == 'text'}}">
    <template is="wx-parser-text" data="{{item}}"/>
  </block>
</template>
